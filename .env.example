# Copy this file to .env and fill in values.
# Never commit real secrets in .env

# Backend
PORT=4000
NODE_ENV=production
MONGODB_URI=mongodb://mongo:27017/hotel_booking

# IMPORTANT: Set this to your public site origin(s).
# Without a domain, use your EC2 Public IPv4 address (and/or Public DNS) like:
# CORS_ORIGIN=http://18.222.123.45,http://ec2-18-222-123-45.us-east-1.compute.amazonaws.com
CORS_ORIGIN=

# SECURITY: generate a long random string
JWT_SECRET=
JWT_EXPIRES_IN=7d

# Change this for production
ADMIN_SIGNUP_CODE=
